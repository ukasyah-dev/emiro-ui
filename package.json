{
  "name": "emiro-ui",
  "version": "0.0.0",
  "description": "Reusable UI component library for React",
  "type": "module",
  "main": "src/index.js",
  "author": "Ukasyah <hi.ukasyah@gmail.com>",
  "license": "MIT",
  "homepage": "https://emiro-ui.ukasyah.workers.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukasyah-dev/emiro-ui"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library"
  ],
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "@eslint/js": "^9.39.2",
    "@react-router/dev": "^7.10.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "isbot": "^5.1.32",
    "react-router": "^7.10.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.54.0"
  }
}
