{
  "name": "emiro-ui",
  "version": "0.0.2",
  "description": "Reusable UI component library for React",
  "type": "module",
  "author": "Ukasyah <hi.ukasyah@gmail.com>",
  "license": "MIT",
  "homepage": "https://emiro-ui.ukasyah.workers.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukasyah-dev/emiro-ui"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library"
  ],
  "scripts": {
    "build": "react-router build",
    "build:lib": "tsdown && cp src/styles.css dist",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./components": {
      "types": "./dist/components.d.mts",
      "import": "./dist/components.mjs"
    },
    "./constants": {
      "types": "./dist/constants.d.mts",
      "import": "./dist/constants.mjs"
    },
    "./styles": {
      "style": "./dist/styles.css"
    },
    "./lib/utils": {
      "types": "./dist/lib/utils.d.mts",
      "import": "./dist/lib/utils.mjs"
    }
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "@eslint/js": "^9.39.2",
    "@mdx-js/rollup": "^3.1.1",
    "@react-router/dev": "^7.10.1",
    "@shikijs/langs": "^3.20.0",
    "@shikijs/rehype": "^3.20.0",
    "@shikijs/themes": "^3.20.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "change-case": "^5.4.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "isbot": "^5.1.32",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1",
    "shiki": "^3.20.0",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.18.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.54.0"
  }
}
